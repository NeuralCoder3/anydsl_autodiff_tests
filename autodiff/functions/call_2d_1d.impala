fn sq(a: f32) -> f32 {
    a*a
}
fn f(x: [f32*2]) -> f32 {
    let x1 = x(0);
    let x2 = x(1);
    x1+sq(3f*x2)
    //sq(x2)
}
fn main() -> i32 {
    let Df = rev_diff(f);
    let yp = Df([3f,2f]);
    let y  = yp(0);
    let yP = yp(1);
    let yPP = yP(1f);
    let r = y as i32;
    let q1 = yPP(0) as i32;
    let q2 = yPP(1) as i32;
    100000 * r + 100*q1 + q2
    // x1 + (3*x2)Â², 1, 18*x2
    // 39, 1, 36
}
