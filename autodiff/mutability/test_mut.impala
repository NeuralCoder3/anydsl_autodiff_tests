
extern fn f(x: f32) -> f32 {
    let mut p = 2f*x;
    let v1 = p+2f;
    let v2 = p+3f;
    p = 3f*x;
    let v3 = p+4f;
    v1*v2+v3
}


fn main() -> i32 {
    let Df = rev_diff(f);
    let yp = Df(4f);
    let y  = yp(0);
    let yP = yp(1);
    let r = y as i32;
    let q = yP(1f) as i32;
    1000 * r + q
}
