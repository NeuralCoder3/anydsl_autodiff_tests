fn f(a: &mut f32) -> f32 {
    if((*a)<=3f) {
        *a=(*a)*(*a)*(*a);
    }else{
        *a=(*a)*(*a);
    }
    *a
}
fn main() -> i32 {
    let Df = rev_diff(f);
    let mut x = 2f;
    let z : &mut f32 = &x;
    let yp = Df(z);
    let y  = yp(0);
    let yP = yp(1);
    let r = y as i32;
    let ypp :f32 = (yP(1f)); // * for ptr pb
    let q = ypp as i32;
    1000 * r + q
}
